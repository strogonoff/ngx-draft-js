{
  "extends": "./tsconfig-esm2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "./tmp/esm5"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "angular-draft.js",
    "flatModuleId": "ngx-draft-js"
  }
}
